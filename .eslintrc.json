{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jest": true
  },
  "extends": ["plugin:flowtype/recommended", "react-app", "prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": ["flowtype", "prettier", "react"],
  "rules": {
    "linebreak-style": ["error", "unix"],
    "no-process-env": "off",
    "no-unused-vars": "off",
    "prettier/prettier": ["error"]
  }
}
