{
  "name": "tidy-tweets",
  "version": "0.1.0",
  "description": "Tidy up your Following list on Twitter.",
  "keywords": [
    "netlify",
    "react",
    "twitter-api"
  ],
  "homepage": "https://tidytweets.org/",
  "bugs": "https://github.com/rlueder/tidytweets/issues",
  "license": "MIT",
  "author": {
    "name": "Rafael LÃ¼der",
    "email": "rlueder@pm.me",
    "url": "https://github.com/rlueder/"
  },
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/rlueder/tidytweets/"
  },
  "scripts": {
    "develop": "cross-env CALLBACK_URL=http://localhost:8888/dashboard netlify dev",
    "flow": "flow",
    "flow-coverage": "flow-coverage-report -i 'src/**/*.js' -t text --threshold 85 --exclude-non-flow",
    "precommit": "lint-staged",
    "prestart": "cp -f ./cert/localhost.pem ./node_modules/webpack-dev-server/ssl || :",
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.10",
    "@reach/router": "^1.3.3",
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.2.1",
    "@testing-library/user-event": "^11.4.2",
    "date-fns": "^2.14.0",
    "localforage": "^1.7.4",
    "react": "^16.13.1",
    "react-copy-write": "^0.8.0",
    "react-countup": "^4.3.3",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-scripts": "3.4.1",
    "twitter-lite": "^0.14.0"
  },
  "devDependencies": {
    "@rescripts/cli": "0.0.14",
    "@rescripts/rescript-env": "0.0.12",
    "@rescripts/rescript-use-babel-config": "0.0.10",
    "cross-env": "^7.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-flowtype": "^5.1.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "flow": "^0.2.3",
    "flow-bin": "^0.127.0",
    "flow-coverage-report": "^0.7.0",
    "flow-typed": "^3.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.10",
    "lodash": "^4.17.15",
    "netlify-cli": "^2.53.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^13.6.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.17.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "rescripts": [
    [
      "use-babel-config",
      ".babelrc"
    ]
  ]
}
